{
  "name": "vite-router",
  "version": "1.0.0",
  "description": "The vite automatic route generation.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build -- --watch --ignore-watch examples",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/treenewlyn/vite-router"
  },
  "homepage": "https://github.com/treenewlyn/vite-router",
  "bugs": "https://github.com/treenewlyn/vite-router/issues",
  "author": "treenew",
  "license": "MIT",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "peerDependencies": {
    "@vue/compiler-sfc": ">=3",
    "vite": ">=2"
  },
  "peerDependenciesMeta": {
    "@vue/compiler-sfc": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.3.3",
    "deep-equal": "^2.0.5",
    "local-pkg": "^0.4.0"
  },
  "devDependencies": {
    "@antfu/utils": "^0.3.0",
    "@types/debug": "^4.1.7",
    "@types/deep-equal": "^1.0.1",
    "@types/node": "^16.11.12",
    "@vue/compiler-sfc": "^3.2.24",
    "cross-env": "^7.0.3",
    "tsup": "^5.10.3",
    "typescript": "^4.5.3",
    "vite": "^2.7.1",
    "vue-router": "^3.5.3"
  }
}
